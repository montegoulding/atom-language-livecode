# LiveCode Builder
##################

'.source.lcb':
  'editor':
    'commentStart': '-- '
    'tabLength': 3
    'foldEndPattern': '^\\s*end\\b'
    'increaseIndentPattern': '^\\s*(((public\\s*)?handler)|if|repeat|else)\\b'
    'decreaseIndentPattern': '^\\s*(end(\\s*(if|repeat|handler))?|else(\\s*if)?)\\b'

